[
    {
        "component": "grid",
        "container": false,
        "columnsPerRow": 2,
        "columns": [
            {
                "className": "result-item-aside two wide",
                "style": {
                    "alignItems": "center"
                },
                "stretched": true,
                "width": "1",
                "items": [
                    {
                        "component": "icon",
                        "dataField": "access_status",
                        "iconSet": {
                            "open": {
                                "src": "https://github.com/Narodni-repozitar/nr-ui/raw/master/public/icons/locks/zamky_open_access.png"
                            }
                        }
                    },
                    {
                        "component": "icon",
                        "dataField": "rights",
                        "fluid": true,
                        "iconSet": {
                            "CC BY-ND": {
                                "src": "https://raw.githubusercontent.com/Narodni-repozitar/nr-ui/master/src/assets/licenses/by-nd.png"
                            }
                        }
                    }
                ]
            },
            {
                "items": [
                    {
                        "component": "result-header",
                        "linksField": "links",
                        "dataField": "title",
                        "as": "a",
                        "size": "large"
                    },
                    {
                        "component": "divided-row",
                        "separator": {
                            "component": "separator",
                            "double": true
                        },
                        "items": [
                            {
                                "component": "list",
                                "dataField": "creators",
                                "horizontal": true,
                                "separator": {
                                    "component": "separator"
                                },
                                "item": {
                                    "component": "authority"
                                }
                            },
                            {
                                "component": "list",
                                "dataField": "contributors",
                                "horizontal": true,
                                "separator": {
                                    "component": "separator"
                                },
                                "item": {
                                    "component": "authority",
                                    "className": "text-gray"
                                }
                            }
                        ]
                    },
                    {
                        "component": "divided-row",
                        "separator": {
                            "component": "separator"
                        },
                        "items": [
                            {
                                "component": "span",
                                "dataField": "publication_year"
                            },
                            {
                                "component": "span",
                                "dataField": "language"
                            }
                        ]
                    },
                    {
                        "component": "row",
                        "items": [
                            {
                                "component": "span",
                                "dataField": "resource_type"
                            },
                            {
                                "component": "defense-status",
                                "dataField": "defended"
                            }
                        ]
                    },
                    {
                        "component": "row",
                        "items": [
                            {
                                "component": "list",
                                "horizontal": true,
                                "divided": true,
                                "dataField": "keywords",
                                "item": {
                                    "component": "link",
                                    "href": "#"
                                }
                            }
                        ]
                    },
                    {
                        "component": "truncated-text",
                        "dataField": "abstract",
                        "lines": 3
                    }
                ]
            }
        ]
    }
]